{
  "name": "im-reading-here",
  "private": true,
  "packageManager": "pnpm@8.15.0",
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "dev": "concurrently \"cd packages/shared && pnpm dev\" \"turbo dev\"",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "test": "turbo test",
    "storybook": "cd apps/web && pnpm storybook",
    "sb": "cd apps/web && pnpm storybook",
    "story": "cd apps/web && pnpm storybook",
    "ui": "cd apps/web && pnpm storybook",
    "build-storybook": "cd apps/web && pnpm build-storybook",
    "build-sb": "cd apps/web && pnpm build-storybook",
    "mobile": "cd apps/mobile && pnpm dev:mobile",
    "mobile:android": "cd apps/mobile && pnpm android",
    "mobile:ios": "cd apps/mobile && pnpm ios",
    "mobile:web": "cd apps/mobile && pnpm web"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "concurrently": "^9.2.1",
    "turbo": "^1.13.4",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
