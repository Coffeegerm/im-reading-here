{
  "name": "im-reading-here",
  "private": true,
  "packageManager": "pnpm@8.15.0",
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "dev": "concurrently \"cd packages/shared && pnpm dev\" \"turbo dev\"",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "start": "turbo start",
    "test": "turbo test",
    "api": "cd apps/api && pnpm dev",
    "web": "cd apps/web && pnpm dev",
    "mobile": "cd apps/mobile && pnpm dev:mobile",
    "db:studio": "cd apps/api && pnpm db:studio",
    "db:migrate": "cd apps/api && pnpm db:migrate",
    "db:push": "cd apps/api && pnpm db:push",
    "db:generate": "cd apps/api && pnpm db:generate",
    "db:seed": "cd apps/api && pnpm db:seed:supabase",
    "storybook": "cd apps/web && pnpm storybook",
    "sb": "cd apps/web && pnpm storybook",
    "story": "cd apps/web && pnpm storybook",
    "ui": "cd apps/web && pnpm storybook",
    "build-storybook": "cd apps/web && pnpm build-storybook",
    "build-sb": "cd apps/web && pnpm build-storybook",
    "mobile:android": "cd apps/mobile && pnpm android",
    "mobile:ios": "cd apps/mobile && pnpm ios",
    "mobile:web": "cd apps/mobile && pnpm web",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:status": "npx supabase status",
    "supabase:restart": "npx supabase stop && npx supabase start",
    "supabase:reset": "npx supabase db reset",
    "supabase:studio": "echo 'Supabase Studio available at: http://127.0.0.1:54323'"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "concurrently": "^9.2.1",
    "supabase": "^2.45.5",
    "tsx": "^4.20.5",
    "turbo": "^1.13.4",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
